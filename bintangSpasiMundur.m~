% bintangSpasiMundur.m : mencetak bintang Spasi Mundur

clear all; close all;

n=input('Masukkan jumlah baris: ');

for i = 1:n 
    % cetak bintang
    for j = 1:n
    fprintf('*');
    end
    
    
    % cetak spasi
    for k = 1:n-i
    fprintf(' ');
    end
    
    n = n - 1;
    fprintf('\n');
end