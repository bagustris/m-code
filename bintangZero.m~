% bintangZero.m : print bintang and Zero pattern

n = input('masukkan jumlah baris: ');

for i=1:n,
    if i==1 || i==n;
        for j=1:n,
        fprintf('*');
        end
    else i>1 & i<n;
        for j=1:n;
            if i==j
            fprintf('*');
            end
    end
    fprintf('\n');
end